<section id="professor-<%= @professor.id %>">
    <h1><%= @professor.name %></h1>
    <% @professor.students.each do |student| %>
      <section id="student-<%= student.id %>">
        <p><%= student.name %></p>
      </section>
    <% end %>
    <p>Average Age: <%= @professor.avg_age_of_students %>
</section>
