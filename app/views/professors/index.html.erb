<section class = "all-professors">

  <% @professors.each do |professor| %>
    <section id="professor-<%= professor.id %>" class = "professor">
          <p>Name: <%= professor.name %></p>
          <p>Age: <%= professor.age %></p>
          <p>Specialty: <%= professor.specialty %></p>
    </section>
  <% end %>

</section>